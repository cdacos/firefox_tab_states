{

  "description": "Save tab states to <profile dir>/browser-extension-data/tab_state@cdacos.com/storage.js",
  "manifest_version": 2,
  "name": "Tab states to JSON file",
  "version": "1.0",

  "applications": {
    "gecko": {
      "id": "tab_state@cdacos.com",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["tab_state.js"]
  },
 
  "permissions": ["storage", "tabs"]

}